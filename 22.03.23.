#include <iostream>
#include <vector>
using namespace std;
int main() {
	vector <string> vlsts{ "USA", "UK", "China", "India", "Canada", "Australia" };
	vector <string> cod{ "+1", "+44", "+86", "+91", "+1", "+61" };

	string atb;
	cout << "Enter country" << endl;
	cin >> atb;
	int i = 0;
	do {
		if (vlsts[i] == atb) cout << cod[i]<<endl;
			i++;
	} while (i < vlsts.size());

	string catb;
	cout << "Enter code" << endl;
	cin >> catb;
	int l = 0;
	do {
		if (cod[l] == catb) cout << vlsts[l];
		l++;
	} while (i < cod.size());

	i = 0;
	l = 0;
	do {
		cout << endl << vlsts[i] << cod[l] << endl;

		i++;
		l++;
	} while (i < vlsts.size()); (l < cod.size());

	vlsts.at(1) = "Estonia";
	cod.at(1) = "+372";
}
