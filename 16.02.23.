#include <iostream>
#include <vector>
using namespace std;
int main() {
	vector <int> k{ 1,2,3,5,7 };
	
		cout << k[0] * k[0]<<endl;
		cout << k[1] * k[1] << endl;
		cout << k[2] * k[2] << endl;
		cout << k[3] * k[3] << endl;
		cout << k[4] * k[4] << endl;
}
