#include <iostream>
using namespace std;
int main() {
	int age;
	cout << "Enter age: ";
	cin >> age;
	if (age >= 21 && age<=100) {
		cout << "You are old enough to enter";
	}

	if (age < 21) {
		cout << "You aren't old enough to enter";
	}

	if (age > 100) {
		cout << "Age error";
	}
	return 0;
}
