#include <iostream>
using namespace std;
bool isNumber(string s)
{
	for (int i = 0; i < s.length(); i++)
		if (isdigit(s[i]) == false)
			return false;
	return true;
}

int main() {
	string pas;

	do {

		cout << "Enter a password" << endl;
		cin >> pas;

		if (isdigit(pas)) { cout << "There are letters!"; };
	} while (pas != "Jekaterina");
	return 0;
}
